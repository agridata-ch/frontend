{
  "name": "frontend",
  "version": "1.0.0-rc.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "cm": "cz",
    "prettier:staged": "npx pretty-quick --staged --restage",
    "prepare": "husky",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "generate:api": "openapi-generator-cli generate -i src/assets/openapi.yaml -g typescript-angular -o ./src/app/shared/openapi --global-property=apis,models,supportingFiles --skip-validate-spec --enable-post-process-file --additional-properties=supportsES6=true --ignore-file-override=./scripts/openapi-ignore.txt"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.9",
    "@angular/compiler": "^19.2.9",
    "@angular/core": "^19.2.9",
    "@angular/forms": "^19.2.9",
    "@angular/platform-browser": "^19.2.9",
    "@angular/platform-browser-dynamic": "^19.2.9",
    "@angular/router": "^19.2.9",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^19.0.1",
    "@angular-devkit/build-angular": "^19.2.10",
    "@angular/cli": "^19.2.10",
    "@angular/compiler-cli": "^19.2.9",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.25.1",
    "@feature-sliced/eslint-config": "^0.1.1",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@openapitools/openapi-generator-cli": "^2.20.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.2",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@tailwindcss/postcss": "^4.1.5",
    "@types/jest": "^29.5.14",
    "commitizen": "4.3.1",
    "commitlint": "^19.8.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns": "^4.1.0",
    "eslint": "^9.25.1",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1",
    "semantic-release": "24.2.3",
    "tailwindcss": "^4.1.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.31.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
