{
  "name": "frontend",
  "version": "1.4.0-rc.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:production": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test:unit": "jest",
    "test:coverage": "jest --coverage --no-cache",
    "test:watch": "jest --watch",
    "cm": "cz",
    "prettier:staged": "npx pretty-quick --staged --restage",
    "prepare": "husky",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "typecheck": "tsc --noEmit",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "i18n:sync": "./src/scripts/sync-translations.sh",
    "i18n:sync:windows": "powershell -ExecutionPolicy Bypass -File \"src/scripts/sync_translations.ps1\" ",
    "commentcheck": "./src/scripts/check-comments.sh",
    "preinstall": "node -e \"require('dotenv').config()\"",
    "api:generate": "openapi-generator-cli generate -i src/assets/openapi.yaml -g typescript-angular -o ./src/entities/openapi --global-property=apis,models,supportingFiles --skip-validate-spec --enable-post-process-file --additional-properties=supportsES6=true --ignore-file-override=./scripts/openapi-ignore.txt",
    "api:fetch:dev": "curl -H \"User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/141.0.0.0 Safari/537.36\" \"https://api.dev.agridata.ch/q/openapi\" -o src/assets/openapi.yaml",
    "api:fetch:local": "curl -H \"User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/141.0.0.0 Safari/537.36\" \"http://localhost:8060/q/openapi\" -o src/assets/openapi.yaml",
    "api:sync:local": "npm run api:fetch:local && npm run api:generate",
    "api:sync:dev": "npm run api:fetch:dev && npm run api:generate",
    "sourcemap:analyze": "ng build --source-map && npx source-map-explorer dist/frontend/**/*.js --no-border-checks"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "20.3.5",
    "@angular/compiler": "20.3.5",
    "@angular/core": "20.3.5",
    "@angular/forms": "20.3.5",
    "@angular/platform-browser": "20.3.5",
    "@angular/platform-browser-dynamic": "20.3.5",
    "@angular/router": "20.3.5",
    "@awesome.me/kit-0b6d1ed528": "^1.0.7",
    "@jsverse/transloco": "8.1.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "20.0.0",
    "@angular-eslint/eslint-plugin": "20.4.0",
    "@angular-eslint/eslint-plugin-template": "20.4.0",
    "@angular-eslint/template-parser": "20.4.0",
    "@angular/build": "20.3.6",
    "@angular/cli": "20.3.6",
    "@angular/compiler-cli": "20.3.5",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/js": "9.37.0",
    "@feature-sliced/eslint-config": "0.1.1",
    "@fortawesome/angular-fontawesome": "3.0.0",
    "@openapitools/openapi-generator-cli": "2.25.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "12.0.0",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@tailwindcss/postcss": "4.1.14",
    "@types/jest": "30.0.0",
    "angular-auth-oidc-client": "20.0.2",
    "commitizen": "4.3.1",
    "commitlint": "20.1.0",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "cz-conventional-changelog": "3.3.0",
    "date-fns": "4.1.0",
    "dotenv": "17.2.3",
    "eslint": "9.37.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-boundaries": "5.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "61.1.4",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-unicorn": "61.0.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-preset-angular": "15.0.2",
    "marked": "16.4.0",
    "ngx-build-plus": "20.0.0",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "pretty-quick": "4.2.2",
    "semantic-release": "25.0.0",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "4.1.14",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
