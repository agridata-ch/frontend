{
  "name": "frontend",
  "version": "1.0.0-rc.88",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test:unit": "jest",
    "test:coverage": "jest --coverage --no-cache",
    "test:watch": "jest --watch",
    "cm": "cz",
    "prettier:staged": "npx pretty-quick --staged --restage",
    "prepare": "husky",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "api:generate": "openapi-generator-cli generate -i src/assets/openapi.yaml -g typescript-angular -o ./src/entities/openapi --global-property=apis,models,supportingFiles --skip-validate-spec --enable-post-process-file --additional-properties=supportsES6=true --ignore-file-override=./scripts/openapi-ignore.txt",
    "typecheck": "tsc --noEmit",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "i18n:sync": "./src/scripts/sync-translations.sh",
    "i18n:sync:windows": "powershell -ExecutionPolicy Bypass -File \"src/scripts/sync_translations.ps1\" ",
    "commentcheck": "./src/scripts/check-comments.sh",
    "licenses": "./src/scripts/generate-third-party-licenses.sh",
    "licenses:check": "./src/scripts/generate-third-party-licenses.sh --check"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "20.2.4",
    "@angular/compiler": "20.2.4",
    "@angular/core": "20.2.4",
    "@angular/forms": "20.2.4",
    "@angular/platform-browser": "20.2.4",
    "@angular/platform-browser-dynamic": "20.2.4",
    "@angular/router": "20.2.4",
    "@jsverse/transloco": "7.6.1",
    "rxjs": "7.8.0",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "20.0.0",
    "@angular-eslint/eslint-plugin": "20.2.0",
    "@angular-eslint/eslint-plugin-template": "20.2.0",
    "@angular-eslint/template-parser": "20.2.0",
    "@angular/build": "20.2.2",
    "@angular/cli": "20.2.2",
    "@angular/compiler-cli": "20.2.4",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint/js": "9.34.0",
    "@feature-sliced/eslint-config": "0.1.1",
    "@fortawesome/angular-fontawesome": "3.0.0",
    "@fortawesome/free-regular-svg-icons": "7.0.1",
    "@fortawesome/free-solid-svg-icons": "7.0.1",
    "@openapitools/openapi-generator-cli": "2.23.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.2",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@tailwindcss/postcss": "4.1.5",
    "@types/jest": "30.0.0",
    "angular-auth-oidc-client": "20.0.2",
    "commitizen": "4.3.1",
    "commitlint": "19.8.0",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "cz-conventional-changelog": "3.3.0",
    "date-fns": "4.1.0",
    "dotenv": "17.2.1",
    "eslint": "9.34.0",
    "eslint-import-resolver-typescript": "4.4.2",
    "eslint-plugin-boundaries": "5.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "54.3.1",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-unicorn": "60.0.0",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "jest-preset-angular": "15.0.0",
    "license-checker": "^25.0.1",
    "marked": "16.2.0",
    "ngx-build-plus": "20.0.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "pretty-quick": "4.1.1",
    "semantic-release": "24.2.3",
    "tailwindcss": "4.1.5",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.42.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
